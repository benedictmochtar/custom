<odoo noupdate="0">
<!--    record ini hanya di-create saat install-->
    <record id="sequence_idea_rentals_seq" model="ir.sequence">
        <field name="name">Rental sequence</field>
        <field name="code">library.rentals</field>
        <field name="prefix">IDEA/%(range_year)s/</field>
        <field eval="3" name="padding"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
        <field eval="True" name="use_date_range"/>
    </record>
</odoo>
